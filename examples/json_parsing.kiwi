base_url = "http://jsonplaceholder.typicode.com"
api_endpoint = "/posts"
api_results = []

repeat 3 as n do
  resp = http::get(base_url, "${api_endpoint}/${n}")
  
  if resp.status == 200 && json::can_parse(resp.body)
    api_results.push(json::parse(resp.body))
  end
end

for res in api_results do
  println res
end