import "@kiwi/argv"
import "@kiwi/string"

fn main(args)
  default_n = 100, n = default_n
  
  if !args.empty()
    n = string::isnumeric(args[0]) ? args[0].to_int() : default_n
  end

  fizzbuzz([n, default_n].max())
end

fn fizzbuzz(n)
  i = 1
  while i <= n do
    if i % 15 == 0
      println("FizzBuzz")
    elsif i % 3 == 0
      println("Fizz")
    elsif i % 5 == 0
      println("Buzz")
    else
      println("${i}")
    end
    i += 1
  end
end

main(argv::get())