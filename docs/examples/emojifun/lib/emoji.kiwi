class Emoji
  private (emap)

  def initialize(emap)
    @emap = emap
  end

  def emotify(input)
    return emoji.emotify(input, @emap)
  end

  def get(name)
    return @emap[name]
  end

  def emojis()
    return @emap.keys()
  end
end

export "emoji"