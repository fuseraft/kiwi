# https://projecteuler.net/problem=6

def sum_of_squares(n)
  return [1 .. n].reduce(0, with (sum, i) do 
    sum += i**2
    return sum
  end)
end

def square_of_sum(n)
  sum = [1 .. n].reduce(0, with (sum, i) do 
    sum += i
    return sum
  end)
  return sum ** 2
end

def difference(n)
  return square_of_sum(n) - sum_of_squares(n)
end

println(difference(100))