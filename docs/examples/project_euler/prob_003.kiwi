# https://projecteuler.net/problem=3

def largest_prime_factor(n)
  factor = 2
  max_prime = 1
  while factor * factor <= n do
    if n % factor == 0
      max_prime = factor
      n /= factor
      while n % factor == 0 do
        n /= factor
      end
    end
    factor += 1
  end

  return __fmax__(max_prime, n)
end

number = 600851475143
println(largest_prime_factor(number))
