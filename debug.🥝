/# a script for experimentation and debugging. #/

repeat 7 as n do
  string_size = 10**n
  println "string_size = 10**${n}              : ${string_size}"
  
  start = time::ticks()
  a = ("a" * string_size).chars()
  println "a = ('a' * string_size).chars()  : ${time::ticksms(time::ticks() - start)}ms"
  
  start = time::ticks()
  c = a[a.size() / 2]
  println "c = a[a.size() / 2]              : ${time::ticksms(time::ticks() - start)}ms"
  println "c.size()                         : ${c.size()}"

  start = time::ticks()
  b = a[1:-1]
  println "b = a[1:-1]                      : ${time::ticksms(time::ticks() - start)}ms"
  println "b.size()                         : ${b.size()}"

  start = time::ticks()
  sum = 0
  for i in a do
    sum += 1
  end
  println "for i in a do sum += 1 end       : ${time::ticksms(time::ticks() - start)}ms"

  println ""
end

/#

