fn greet(name)
  println name
end

a = "Hello, World!"
b = [1, 2, 3, ["a", "b"]]
c = b[0]
c += 1

greet(a)

a = {
  "a": true, 
  "b": {
    "c": [1, 2, {"key": 3}]
  }
}

a.x.set(0, a.x.get(2))

a = {}
a.x = 5

a.b.c = [1]

a.x = [1..5].size()

a = [1,2][0]
