fn test(a, b)
  c = { "a": a, "b": b }
  println "object:          ${c}"
  
  d = '{"a": null, "b": null}'
  println "raw_string:      ${d}"
  
  e = d.to_hash()
  println "deserialized:    ${e}"

  e.a = false
  e.b = 'hello'
  println "assigned values: ${e}"

  e.a = null
  e.b = null
  println "assigned null:   ${e}"

  e = null
  println "assigned null:   ${e}"

  if e == null
    println "null check"
  end

  e = 0

  if e != null
    println "null check"
  end

  # another null check
  if !!!null
    println "hello world"
  end
end

test(null, null)