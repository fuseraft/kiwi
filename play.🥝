# This ğŸ¥ script is for experimentation.

l_a = [1, 3, 5, true, false]
l_b = [1, true, 6, 5, 8, 9]

m_a = []
m_b = []

for a in l_a do
  if l_b.contains(a)
    m_a << a
  end
end

for b in l_b do
  if l_a.contains(b)
    m_b << b
  end
end

println "l_a join l_b = ${m_a}"
println "l_b join l_a = ${m_b}"